{
  "brains": {
    "cacheTime": 86400000
  },
  "github": {
    "api": {
      "version": "3.0.0",
      "protocol": "https",
      "host": "api.github.com"
    },
    "protocol": "https",
    "host": "github.com",
    "token": ""
  },
  "npm": {
    "hostname": "http://npmjs.org",
    "options": {
      "registry": "http://isaacs.iriscouch.com/registry"
    }
  },
  "site": {
    "hostname": "https://mohi.io"
  },
  "bitbucket": {
    "protocol": "https",
    "host": "bitbucket.org",
    "username": "",
    "password": ""
  },
  "dependencies": {
    "excluded": [
      "com.github.zenedith:gradle-versions-plugin"
    ]
  },
  "gradle": {
    "gradleBuildDir": "var/mohi/gradle",
    "gradlewVendorDir": "vendor/gradlew",
    "decorateGradleBuildScript": "vendor/scripts/decorateVersion.sh",
    "decorateGradleProjectInfoTask": "vendor/scripts/decorateProjectInfoTask.sh"
  },
  "processingQueue": {
    "timeout": 3600000,
    "concurrency": 2
  },
  "newrelic": {
    "license_key": "",
    "logging": "info"
  },
  "storage": {
    "type": "elasticsearch",
    "elasticsearch": {
      "hosts": [
        {
          "protocol": "http",
          "host": "IP",
          "port": 9200,
          "weight": 1
        }
      ],
      "sniffOnStart": true,
      "sniffInterval": 60000,
      "requestTimeout": 5000,
      "minSockets": 1,
      "log": {
        "type": "stdio",
        "level": "trace"
      }
    }
  }
}